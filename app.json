{
  "expo": {
    "name": "siga-mobile-app",
    "slug": "siga-mobile-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo/app-icon.png",
    "userInterfaceStyle": "light",
    "scheme": "siga-mobile-app",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "version": "1.0.0",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/app-icon.png"
      },
      "package": "com.carlos.fiori44.sigamobileapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "53bdab80-fbd4-4277-b02e-98e7e729d5af"
      }
    }
  }
}
